okemon({name}) {\n    // console.log({name}); // wat krijg je terug? alleen de naam zie app.js\n\n    const [singlePokemon, setSinglePokemons] = useState({});\n    const [moves, setMoves] = useState([]);\n    const [weight, setWeight] = useState([]);\n    const [sprites, setSprites] = useState([]);\n    // const [abilities, setAbilities] =useState({});\n\n    useEffect(() => {\n        async function fetchOneData() {\n            try {\n                const {data} = await axios.get(`https://pokeapi.co/api/v2/pokemon/${name}`)\n\n                setSinglePokemons(data);\n                // console.log(data);\n                setMoves(data.moves.length);\n                setWeight(data.weight);\n                setSprites(data.sprites.front_default);\n\n            } catch (e) {\n                console.log(e)\n            }\n        }\n\n        fetchOneData();\n    }, [url])\n\n\n    return (\n\n        <div className=\"single-card\">\n            {singlePokemon && <h3>{singlePokemon?.name}</h3>}\n\n                <img src={sprites} alt={singlePokemon.name}/>\n                <p>moves: {moves}</p>\n                <p>weight: {weight}</p>\n\n\n\n\n\n        </div>\n\n    )\n\n}\n\nexport default Pokemon;\n\n\n\n\n\n\n\n\n\n\n\n\n\n",{"ruleId":"26","replacedBy":"27"},{"ruleId":"28","replacedBy":"29"},{"ruleId":"30","severity":1,"message":"31","line":6,"column":13,"nodeType":"32","messageId":"33","endLine":6,"endColumn":16},{"ruleId":"30","severity":1,"message":"34","line":12,"column":12,"nodeType":"32","messageId":"33","endLine":12,"endColumn":19},{"ruleId":"30","severity":1,"message":"35","line":12,"column":21,"nodeType":"32","messageId":"33","endLine":12,"endColumn":34},{"ruleId":"36","severity":1,"message":"37","line":38,"column":8,"nodeType":"38","endLine":38,"endColumn":13,"suggestions":"39"},"no-native-reassign",["40"],"no-negated-in-lhs",["41"],"no-unused-vars","'url' is defined but never used.","Identifier","unusedVar","'loading' is assigned a value but neve